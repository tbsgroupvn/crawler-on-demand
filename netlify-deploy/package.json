{
  "name": "crawler-on-demand-netlify",
  "version": "2.0.0",
  "description": "Professional web scraping solution with FastAPI, Celery, Redis & Excel export - Netlify Deployment",
  "homepage": "https://crawlall.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/tbsgroupvn/crawler-on-demand"
  },
  "bugs": {
    "url": "https://github.com/tbsgroupvn/crawler-on-demand/issues"
  },
  "keywords": [
    "web-scraping",
    "crawler",
    "fastapi",
    "celery",
    "redis",
    "docker",
    "netlify",
    "demo"
  ],
  "author": "TBS Group Vietnam",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "echo 'Building static assets for deployment'",
    "dev": "python -m http.server 8888",
    "start": "python -m http.server 8000",
    "test": "echo 'Running deployment tests' && exit 0",
    "validate": "node scripts/validate-deployment.js",
    "optimize": "node scripts/optimize-assets.js",
    "deploy": "echo 'Deploying to Netlify'",
    "preview": "netlify dev",
    "lint": "echo 'Linting deployment files'"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.41.1",
    "netlify-cli": "^17.0.0",
    "html-minifier": "^4.0.0",
    "clean-css": "^5.3.2",
    "terser": "^5.19.4"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "netlify": {
    "functions": "functions",
    "publish": ".",
    "command": "npm run build"
  },
  "deployment": {
    "platform": "netlify",
    "environment": "production",
    "features": [
      "redirects",
      "headers",
      "forms",
      "edge-functions"
    ]
  },
  "performance": {
    "optimization": true,
    "compression": true,
    "caching": true,
    "minification": true
  }
} 